{
    "bail": true,
    "browser": true,
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
        "build",
        "/node_modules/"
    ],
    "coverageReporters": [
        "html",
        "json"
    ],
    "errorOnDeprecated": true,
    "globals": {},
    "moduleDirectories": [
        "node_modules"
    ],
    "moduleFileExtensions": [
        "js"
    ],
    "notify": true,
    "notifyMode": "always",
    "resetMocks": true,
    "testEnvironment": "jest-environment-jsdom",
    "testMatch": [
        "**/__tests__/**/*.js?",
        "**/?(*.)+(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
        "/node_modules/",
        "build"
    ],
    "verbose": true
}
